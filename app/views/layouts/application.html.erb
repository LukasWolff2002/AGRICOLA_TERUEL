    <%= javascript_include_tag "application", "data-turbo-track": "reload", type: "module" %>
    
   
    <!-- Bootstrap CSS y JS -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
